<!DOCTYPE html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/styles/getinvolved.css"/>
    <link rel="stylesheet" type="text/css" href="/styles/fullpage.css"/>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script type="text/javascript" src="/scripts/scrolloverflow.min.js"></script>
    <script type="text/javascript" src="/scripts/fullpage.js"></script>
</head>
<body>
    <div id="fullpage">
        <div id="fixed-wrapper">
            <%- include('../../partials/main_nav'); %>
        </div>
        <div class="section" id="athome-intro">
            <img src="/imgs/get_involved/home/at_home.svg">
        </div>
        <div class="section">
            <div class="container clr-home">
                <% cards.forEach(function(card) { %>
                    <div class="card">
                        <a href="<%= card.link %>">
                        <img src="/imgs/get_involved/home/<%= card.img %>">
                            <div class="desc">
                                <%= card.desc %>
                            </div>
                        </a>
                    </div>
                <% }) %>
            </div>
        </div>
    </div>
    <script>
            new fullpage('#fullpage', {
                //options here
                navigation: true,
                slidesNavigation: true,
                scrollOverflow: true,
            });
    </script>

</body>